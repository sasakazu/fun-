

<%= render 'shared/header' %>

<div class="container">

  <h2>音源登録</h2>




  <%= form_for @music do |f| %>

    <div class="field">
      <%= f.label :タイトル %>
      <%= f.text_field :title %>
    </div>

    <div class="field">
      <%= f.file_field :music %>
    </div>


  <%= f.submit '送信' %>

  <% end %>




</div>






<%= render 'shared/footer' %>
